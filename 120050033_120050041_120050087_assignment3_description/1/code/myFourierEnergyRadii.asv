function [energy_ratio] = myFourierEnergy(s_fourier, radii)
    power = s_fourier.*conj(s_fourier);
    
    [rows, cols] = size(s_fourier);
    cr = ceil(rows/2);
    cc = ceil(cols/2);
    
    filtered = zeros(rows, cols);
    for i = 1:rows
        for j = 1:cols
            distance = pdist([i, j; cr, cc], 'euclidean');
            if (distance <= radii)
                filtered(i,
            end
        end
    end
    
end